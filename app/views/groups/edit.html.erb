<%= render 'manage_group' %>

<script type="text/javascript">
	$(function () {
		$("#page_num").val('1');
		$('#users').toggleClass('none', false);
		<% @users.each do |user| %>
			$("#users_to_add").append(hidden_template(<%= {
				twitter_id_str: user['id_str']
			}.to_json.html_safe %>));

			$("#current_members").append(template(<%= {
				profile_image_url: user['profile_image_url'], 
				name: user['name'], 
				id_str: user['id_str'],
				location: user['location']
			}.to_json.html_safe %>));
		<% end %>
	})
</script>